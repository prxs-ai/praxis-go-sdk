@startuml
title DSL Processing Example

note top: Input CALL write_file document.txt "Important data"

rectangle "Tokenization" {
  card CALL
  card write_file
  card "document.txt"
  card "Important data"
}

rectangle "AST Building" {
  object CallNode {
    type = call
    value = write_file
  }
  object Arg1 {
    type = literal
    value = document.txt
  }
  object Arg2 {
    type = string
    value = Important data
  }
  CallNode --> Arg1
  CallNode --> Arg2
}

@enduml